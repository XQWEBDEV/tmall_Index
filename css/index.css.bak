.c-main .c-main-end{
	height:45px;
	margin:15px 575px;
	background:url("../images/end.png") no-repeat center/100% 100%;
}
/*底部区域*/
#footer{
	background-color:#fff;
	border-top:1px solid #ddd;
}
#footer .f-tmall-ensure{
	padding:30px 0;
	margin-bottom:30px;
	border-bottom:1px solid #ddd;
}
#footer .f-tmall-ensure a{
	display:block;
	float:left;
	width:297.5px;
	height:50px;
	padding:25px 0;
	color:#333;
	background-color:white;
}
.f-tmall-ensure>a>i{
	font-size:50px;
	color:#f00;
	font-weight:500;
	margin-left:50px;
	margin-right:20px;
}
.f-tmall-ensure>a strong{
	font-size:18px;
	width:177px;
	margin-bottom:5px;
}
.f-tmall-ensure>a span{
	color:#999;
}
#footer .f-tmall-desc .tmall-desc-item{
	float:left;
	width:186px;
	padding-left:52px;
}
#footer .f-tmall-desc{
	border-bottom:2px solid red;
}
.f-tmall-desc .f-td-list{
	padding-bottom:30px;
	background:url("../images/TB1dl.png") no-repeat 65px bottom/70px 30px;
}
.f-tmall-desc .tmall-desc-item h3{
	color:#646464;
	font-size:16px;
	margin-bottom:5px;
	font-weight:bold;
}
.f-tmall-desc .tmall-desc-item a{
	color:#999;
	font-size:12px;
	line-height:1.5em;
}
#footer .tmall-copyright{
	background-color:#000;
	font-size:12px;
}
.tmall-copyright .footer-tmallinfo{
	line-height:40px;
}
.tmall-copyright .footer-tmallinfo a{
	margin-right:5px;
}
.tmall-copyright .tmall-otherlink{
	padding:10px 0;
	height:12px;
	font-size:12px;
}
#footer .tmall-copyright a{color:#fff;}
.tmall-copyright .tmall-otherlink a{
	padding-right:3px;
}
.tmall-copyright .tmall-otherlink a:after{
	display:inline-block;
	content:"";
	vertical-align:-2px;
	margin-left:5px;
	width:1px;
	height:12px;
	background-color:#fff;
}
.tmall-copyright .tmall-otherlink .afterw0:after{
	display:inline-block;
	content:"";
	width:0;
}
.tmall-copyright .footer-copyright{
	color:#a4a4a4;
	line-height:20px;
}
#footer .tmall-copyright .footer-copyright a{
	color:#a4a4a4;
	margin-right:65px;
}
.tmall-copyright .footer-copyright b{
	color:#686868;
	font-weight:normal;
}
#footer .tmall-copyright .footer-copyright p{
	padding:10px 0;
}
.tmall-copyright .footer-copyright img{
	vertical-align:middle;
}
/*左侧固定导航*/
#wrap .fixed-left-nav{
	position:fixed;
	top:30%;
	left:50%;
	margin-left:-670px !important;
	width:35px;
	text-align:center;
}
#wrap .fixed-left-nav a{
	display:block;
	font-size:12px;
	line-height:12px;
	padding:6px 0;
	color:#fff;
	height:23px;
	background-color:#999;
	margin-bottom:1px;
}
.fixed-left-nav a i{
	font-size:14px;
	line-height:10px;
}
#wrap .fixed-left-nav .f-l-lh36{
	line-height:24px;
}
#wrap .fixed-left-nav .bgc-f04{
	background-color:#f04;
}
#wrap .fixed-left-nav .c-3c0:hover{
	background-color:#3c0;
}
#wrap .fixed-left-nav .c-000:hover{
	background-color:#000;
}
#wrap .fixed-left-nav .c-f69:hover{
	background-color:#f69;
}
#wrap .fixed-left-nav .c-3fc:hover{
	background-color:#3fc;
}

/*固定右侧导航*/
#wrap .fixed-right-nav{
	position:fixed;
	right:0;
	top:0;
	z-index:10000;
	background-color:#000;
	width:35px;
	height:100%;
	text-align:center;
	color:#fff;
}
.fixed-right-nav .fx-rnav-list{
	margin-top:60px;
}

.fixed-right-nav  i{
	vertical-align:-2px;
	color:#fff;
	line-height:0;
	font-size:24px;
}
.fixed-right-nav .fx-rbnav-list{
	position:absolute;
	width:35px;
	bottom:5px;
}
.fixed-right-nav .fx-rbnav-list i{
	font-size:20px;
}
.fixed-right-nav .fx-rl-item{
	margin:8px 0;
	line-height:35px;
	cursor:pointer;
}
.fixed-right-nav .tmall-cart{
	margin:0px 4px;
	line-height:1em;
	padding-top:20px;
	padding-bottom:10px;
	border-top:1px solid #999;
	border-bottom:1px solid #999;
}
.fixed-right-nav .tmall-cart i{
	color:red;
}
.fixed-right-nav .fx-rnav-list .rl-item-cart{
	display:block;
	padding:5px 4px;
	font-size:12px;
	line-height:1.2em;
}
.fixed-right-nav .fx-rbnav-list .fx-rl-item{
	margin:0;
}
.fixed-right-nav .fx-rl-item:hover{
	background-color:#ff0036;
}
.fixed-right-nav .fx-rl-item:hover .tmall-cart{
	border-color:#ff0036;
} 
.fixed-right-nav .fx-rl-item:hover .tmall-cart i{
	color:#fff;
}

