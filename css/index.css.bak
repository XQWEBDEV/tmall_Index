
#wrap{
	max-width:1980px;
	background:#f5f5f5 url("../images/body-bg.jpg") center top no-repeat;
}
#header .header-topnav{
	background-color:#f2f2f2;
	color:#999;
}
#header .header-topnav{
	position:relative;
	z-index:10;
	font-size:12px;
}
.header-topnav .h-t-left>li,
.header-topnav .h-t-right>li{
	float:left;
	line-height:40px;
}
.header-topnav li a{
	color:#999;
}
.header-topnav .h-t-right>li{
	position:relative;
	padding:0 14px;
}
.header-topnav i{
	vertical-align:1px;
	color:red;
	font-size:12px;
	margin-right:3px;
}
.header-topnav .down{
	border:1px solid rgba(0,0,0,0);
	border-width:0 1px;
}
.header-topnav .down:after{
	position:absolute;
	top:1.5em;
	right:4px;
	content:"";
	width:0;
	height:0;
	border:4px solid rgba(0,0,0,0);
	border-top-color:#999;
}
.header-topnav .down:hover{
	background-color:#fff;
	border-color:#e5e5e5;
	text-decoration:underline;
}
.header-topnav .down:hover .down-list{
	display:block;
}
.header-topnav .h-t-left>li:hover>a,
.header-topnav .h-t-right>li:hover>a{
	color:#f00;
}
.header-topnav .h-t-right .down-list{
	position:absolute;
	top:39px;
	display:none;
	z-index:-1;
	line-height:20px;
	padding:8px 10px;
	background-color:#fff;
	border:1px solid #e5e5e5;
}
.header-topnav .h-t-right .h-m-taobao{
	left:-1px;
	width:72px;
}
.header-topnav .h-t-right .h-m-collect{
	left:-1px;
	width:61px;
}
.header-topnav .h-t-right .h-business-support{
	right:-1px;
	width:122px;
	padding-top:0;
}
.h-t-right .h-business-support li{
	padding:8px 0;
	border-bottom:1px dashed #ddd;
}
.h-t-right .h-business-support .support-rg{
	margin-left:20px;
}
.header-topnav .h-t-right  .h-phone-drop{
	position:absolute;
	top:35px;
	left:-42.5px;
	display:none;
	background-color:#fff;
	box-shadow:0 0 10px #000;
}
.header-topnav  .h-t-right .h-site-nav{
	right:-1px;
	width:1012px;
	padding:20px 0;
}

.header-topnav  .h-t-right .h-mobile-phone:hover .h-phone-drop{
	display:block;
}
.h-t-right .h-site-nav .h-site-item{
	float:left;
	padding:0 30px;
	height:170px;
	border-right:1px solid #eee;
}
.header-topnav .h-t-right .h-site-nav h3{
	font-size:16px;
	color:#000;
}
.header-topnav .h-t-right .site-hot h3{
	color:#f56a00;
}
.header-topnav .h-t-right .site-market h3{
	color:#2263d4;
}
.header-topnav .h-t-right .down-list a:hover{
	color:#f00;
	text-decoration:underline;
}
.h-t-right .h-site-nav .h-site-item a:hover{
	color:#000;
}
.header-topnav .h-t-right .site-hot a:hover{
	color:#f56a00;
}
.header-topnav .h-t-right .site-market a:hover{
	color:#2263d4;
}
.h-site-nav .h-site-nav .h-site-item h3{
	font-size:16px;
}
.h-t-right .h-site-nav .bor0{
	border:0;
}
.h-site-nav .h-site-item>ul{
	float:left;
	margin-right:40px;
}
.h-site-nav .h-site-item .mrg0{
	margin-right:0;
}
.h-site-nav .h-site-item>ul>li{
	line-height:30px;
}

#header .h-content{
	position:relative;
	/* background-color:yellow; */
}
